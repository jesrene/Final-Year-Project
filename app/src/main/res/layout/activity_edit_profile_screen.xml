                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:ellipsize="end"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@string/lbl_account" />

            <FrameLayout
                android:id="@+id/frameUploadphoto"
                android:layout_width="@dimen/_170pxh"
                android:layout_height="@dimen/_185pxv"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_108pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_marginEnd="@dimen/_108pxh">

                <View
                    android:id="@+id/viewUploadeclipse"
                    android:layout_width="@dimen/_160pxh"
                    android:layout_height="@dimen/_151pxh"
