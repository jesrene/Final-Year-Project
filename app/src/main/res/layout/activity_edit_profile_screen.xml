                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_yukii_gmail_com" />
            </LinearLayout>

            <RelativeLayout
                android:id="@+id/linearPassword"
                style="@style/groupStylewhite_A700cornerRadius"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtPASSWORD"
                    style="@style/txtWorkSansromanregular18"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_weight="0.5"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_password" />

                <TextView
                    android:id="@+id/txtFour"
                    style="@style/txtWorkSansromanmedium22"
                    android:layout_width="101dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="126dp"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_toEndOf="@id/txtPASSWORD"
                    android:layout_weight="0.5"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@string/lbl2" />
            </RelativeLayout>

