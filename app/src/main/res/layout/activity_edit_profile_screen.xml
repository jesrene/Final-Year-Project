                    android:background="@drawable/rectangle_bg_bluegray_100_radius_77_42" />

                <LinearLayout
                    android:id="@+id/linearColumnuploadphotoic"
                    android:layout_width="116dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_marginEnd="@dimen/_32pxh"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageUploadphotoic"
                        android:layout_width="@dimen/_46pxh"
                        android:layout_height="@dimen/_42pxh"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_uploadphotoic"

                         />

                    <TextView
                        android:id="@+id/txtUploadyourpho"
                        style="@style/txtWorkSansromanregular16"
                        android:layout_width="match_parent"
                        android:layout_height="43dp"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:lineSpacingExtra="@dimen/_2pxh"
                        android:textSize="@dimen/_12pxh"
                        android:text="@string/msg_upload_your_ph" />
                </LinearLayout>
            </FrameLayout>

            <LinearLayout
                android:id="@+id/linearName"
                style="@style/groupStylewhite_A700cornerRadius"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginEnd="@dimen/_20pxh"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtNAME"
                    style="@style/txtWorkSansromanregular18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_weight="0"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_4pxh"
